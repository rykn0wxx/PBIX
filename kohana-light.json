{
  "name": "Kohana Theme",
  "background": "#F3F3F5",
  "foreground": "#050709",
  "tableAccent": "#00E6A2",
  "dataColors": [ "#4C6D9D", "#393D3F", "#ed8936", "#41AB5D", "#FF533D", "#984EA3", "#2683C6", "#44546A", "#38b2ac","#9D7C4C" ],
  "visualStyles": {
    "*": {
      "*": {
        "*": [
          {
            "responsive": false,
            "wordWrap": false,
            "fontFamily": "Segoe UI"
          }
        ],
        "background": [
          {
            "show": true,
            "color": { "solid": { "color": "#FFF" } },
            "transparency": 0
          }
        ],
        "title": [
          {
            "show": true,
            "fontColor": { "solid": { "color": "#1D2025" } },
            "alignment": "left",
            "fontSize": 13,
            "fontFamily": "Segoe (Bold)",
            "background": { "solid": { "color": "" } }
          }
        ],
        "legend": [
          {
            "show": true,
            "showTitle": false,
            "labelColor": {
              "solid": {
                "color": "#77747B"
              }
            },
            "fontSize": 10
          }
        ],
        "labels": [
          {
            "show": false,
            "fontSize": 11,
            "fontFamily": "Trebuchet MS",
            "color": {
              "solid": {
                "color": "#0D3A5F"
              }
            },
            "labelDisplayUnits": 0,
            "labelOrientation": "Horizontal",
            "labelPosition": "OutsideEnd",
            "enableBackground": true,
            "backgroundColor": {
              "solid": {
                "color": "#E6E6E6"
              }
            },
            "backgroundTransparency": 50
          }
        ],
        "categoryAxis": [
          {
            "show": true,
            "showAxisTitle": false,
            "axisStyle": "showTitleOnly",
            "titleFontSize": 10,
            "fontSize": 10,
            "titleColor": {
              "solid": {
                "color": "#0D3A5F"
              }
            },
            "preferredCategoryWidth": 20,
            "maxMarginFactor": 20,
            "innerPadding": 10,
            "concatenateLabels": false
          }
        ],
        "valueAxis": [
          {
            "show": true,
            "fontSize": 10,
            "titleColor": {
              "solid": {
                "color": "#0D3A5F"
              }
            },
            "showAxisTitle": false,
            "axisStyle": "showTitleOnly",
            "titleFontSize": 10,
            "gridlineShow": true,
            "gridlineThickness": 1,
            "gridlineStyle": "dashed",
            "gridlineColor": {
              "solid": {
                "color": "#E6E6E6"
              }
            }
          }
        ],
        "lockAspect": [
          {
              "show": false
          }
        ],
        "stylePreset": [
          {
            "name": "Default"
          }
        ],
        "lineStyles": [
          {
            "shadeArea": false,
            "strokeWidth": 2,
            "strokeLineJoin": "bevel",
            "showMarker": true,
            "markerShape": "circle",
            "markerSize": 7
          }
        ],
        "border": [
          {
            "color": { "solid": { "color": "#E6E6E6" }},
            "roundEdge": 8
          }
        ]
      }
    },
    "page": {
      "*": {
        "background": [
          {
            "color": { "solid": { "color": "#FAFBFC" } },
            "transparency": 0
          }
        ],
        "outspace": [
          {
            "color": { "solid": { "color": "#E4E5E6" } },
            "transparency": 0
          }
        ]
      }
    },
    "basicShape": {
      "*": {
        "title": [
          {
            "show": false
          }
        ],
        "line": [
          {
            "lineColor": {
              "solid": {
                "color": "#E6E6E6"
              }
            },
            "transparency": 0,
            "weight": 1,
            "roundEdge": 8
          }
        ],
        "background": [
          {
            "show": false,
            "color": { "solid": { "color": "#FFF" }},
            "transparency": 0
          }
        ],
        "fill": [
          {
            "show": true,
            "fillColor": {
              "solid": {
                "color": "#FFF"
              }
            },
            "transparency": 0
          }
        ],
        "rotation": [
          {
            "angle": 0
          }
        ]
      }
    },
    "pivotTable": {
      "*": {
        "*": [
          {
            "fontColor": { "solid": { "color": "#3170b7" }}
          }
        ],
        "title": [{
          "show": false
        }],
        "grid": [{
          "outlineWeight": 3,
          "gridVerticalColor": { "solid": { "color": ""}},
          "gridHorizontalColor": { "solid": { "color": ""}},
          "gridVerticalWeight": 1,
          "gridHorizontalWeight": 1,
          "gridVertical": false,
          "gridHorizontal": false,
          "rowPadding": 4,
          "textSize": 14,
          "imageHeight": 100
        }],
        "subTotals": [{
          "columnSubtotals": false,
          "fontColor": { "solid": { "color": "#0D3A5F" }},
          "applyToHeaders": false
        }],
        "total": [{
          "columnSubtotals": false,
          "fontColor": { "solid": { "color": "#0D3A5F" }},
          "backColor": { "solid": { "color": "#edf2f7" }},
          "textSize": 12,
          "fontFamily": "Segoe UI,Segoe (Bold)",
          "applyToHeaders": true
        }],
        "columnHeaders": [{
          "fontColor": { "solid": { "color": "#0D3A5F"}},
          "autoSizeColumnWidth": true,
          "alignment": "Auto",
          "fontSize": 12,
          "wordWrap": true,
          "backColor": { "solid": { "color": "#edf2f7" }},
          "stepped": true,
          "steppedLayoutIndentation": 25,
          "urlIcon": true
        }],
        "rowHeaders": [{
          "fontColor": { "solid": { "color": "#0D3A5F"}},
          "wordWrap": true,
          "backColor": { "solid": { "color": "#edf2f7" }},
          "stepped": true,
          "steppedLayoutIndentation": 25,
          "urlIcon": true,
          "fontSize": 11
        }],
        "values": [{
          "fontColorPrimary": { "solid": { "color": "#0D3A5F" }},
          "backColorPrimary": { "solid" : { "color": "#F5F5F5" }},
          "fontColorSecondary": { "solid": { "color": "#0D3A5F" }},
          "backColorSecondary": { "solid" : { "color": "#FAFAFA" }},
          "wordWrap": true,
          "fontFamily": "Trebuchet MS",
          "fontSize": 12,
          "urlIcon": true
        }]
      }
    },
    "card": {
      "*": {
        "labels": [{
          "labelDisplayUnits": "0",
          "labelPrecision": 0,
          "fontSize": 30,
          "fontFamily": "wf_standard-font",
          "color": { "solid": { "color": "#0D3A5F" }}
        }],
        "categoryLabels": [{
          "show": true,
          "fontSize": 11,
          "color": { "solid": { "color": "#3170b7" }}
        }],
        "wordWrap": [{
          "show": true
        }],
        "title": [{
          "show": false,
          "fontSize": 12
        }],
        "border": [{
          "show": true,
          "color": { "solid": { "color": "#E6E6E6" }},
          "radius": 4
        }],
        "background": [
          {
            "color": { "solid": { "color": "" }},
            "transparency": 0
          }
        ]
      }
    },
    "multiRowCard": {
      "*": {
        "title": [
          {
            "show": false
          }
        ],
        "dataLabels": [{
          "color": { "solid": { "color": "#0D3A5F" }},
          "fontSize": 20,
          "fontFamily": "wf_standard-font"
        }],
        "categoryLabels": [{
          "show": true,
          "color": { "solid": { "color": "#3170b7" }},
          "fontSize": 10
        }],
        "card": [{
          "outline": "None",
          "outlineColor": { "solid": { "color": "#E6E6E6" }},
          "outlineWeight": 1,
          "barShow": true,
          "barWeight": 4,
          "barColor": { "solid": { "color": "#00E6A2" }},
          "cardPadding": 10
        }]
      }
    },
    "slicer": {
      "*": {
        "general": [
          {
            "outlineColor": {
              "solid": {
                "color": "#77747B"
              }
            },
            "outlineWeight": 1,
            "orientation": "vertical",
            "responsive": true,
            "fontColor": {
              "solid": {
                "color": "#3170b7"
              }
            }
          }
        ],
        "data": [
          {
            "mode": "List",
            "relativeRange": "",
            "relativePeriod": ""
          }
        ],
        "selection": [
          {
            "selectAllCheckboxEnabled": false,
            "singleSelect": true
          }
        ],
        "header": [
          {
            "show": true,
            "fontColor": {
              "solid": {
                "color": "#0D3A5F"
              }
            },
            "outline": "BottomOnly",
            "textSize": 11
          }
        ],
        "items": [
          {
            "fontColor": {
              "solid": {
                "color": "#3170b7"
              }
            },
            "background": {
              "solid": {
                "color": ""
              }
            },
            "outline": "None",
            "textSize": 11
          }
        ],
        "title": [
          {
            "show": false
          }
        ]
      }
    },
    "kpi": {
      "*": {
        "indicator": [
          {
            "indicatorDisplayUnits": 0,
            "indicatorPrecision": 1,
            "fontSize": 40
          }
        ],
        "trendline": [
          {
            "show": true
          }
        ],
        "goals": [
          {
            "showGoal": true,
            "showDistance": true
          }
        ],
        "status": [
          {
            "direction": "Positive",
            "goodColor": {
              "solid": {
                "color": "#00c292"
              }
            },
            "neutralColor": {
              "solid": {
                "color": "#ffcd56"
              }
            },
            "badColor": {
              "solid": {
                "color": "#f96262"
              }
            }
          }
        ]
      }
    },
    "tableEx": {
      "*": {
        "grid": [
          {
            "gridVertical": false,
            "gridVerticalWeight": 1,
            "gridHorizontal": true,
            "gridHorizontalWeight": 1,
            "rowPadding": 3,
            "outlineWeight": 2,
            "textSize": 10
          }
        ],
        "columnHeaders": [
          {
            "fontColor": {
              "solid": {
                "color": "#0D3A5F"
              }
            },
            "backColor": {
              "solid": {
                "color": ""
              }
            },
            "outline": "BottomOnly",
            "autoSizeColumnWidth": false,
            "fontSize": 11,
            "alignment": "Left",
            "wordWrap": true
          }
        ],
        "values": [
          {
            "fontColorPrimary": {
              "solid": {
                "color": "#3170b7"
              }
            },
            "fontColorSecondary": {
              "solid": {
                "color": "#3170b7"
              }
            },
            "outline": "None",
            "urlIcon": true,
            "wordWrap": true,
            "fontSize": 10
          }
        ],
        "total": [
          {
            "totals": true,
            "fontColor": {
              "solid": {
                "color": "#0D3A5F"
              }
            },
            "backColor": {
              "solid": {
                "color": "#edf2f7"
              }
            },
            "outline": "TopOnly",
            "fontSize": 11
          }
        ]
      }
    }
  }
}
